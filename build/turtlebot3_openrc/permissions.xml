<?xml version="1.0" encoding="utf-8"?>
<dds>
  <permissions>
    <grant name="turtlebot3_openrc">
      <subject_name>CN=turtlebot3_openrc</subject_name>
      <validity>
        <not_before>2013-10-26T00:00:00</not_before>
        <not_after>2023-10-26T22:45:30</not_after>
      </validity>
      <allow_rule>
        <domains>
          <id>0</id>
        </domains>
        <subscribe>
          <topics>
            <topic>rt/cmd_vel</topic>
            <topic>rt/motor_power</topic>
            <topic>rt/reset</topic>
            <topic>rt/time_sync</topic>
          </topics>
        </subscribe>
        <publish>
          <topics>
            <topic>rt/imu</topic>
            <topic>rt/joint_states</topic>
            <topic>rt/sensor_state</topic>
            <topic>rt/version_info</topic>
          </topics>
        </publish>
      </allow_rule>
      <default>DENY</default>
    </grant>
  </permissions>
</dds>
