<?xml version="1.0" encoding="utf-8"?>
<dds>
  <permissions>
    <grant name="time_sync">
      <subject_name>CN=time_sync</subject_name>
      <validity>
        <not_before>2013-10-26T00:00:00</not_before>
        <not_after>2023-10-26T22:45:30</not_after>
      </validity>
      <allow_rule>
        <domains>
          <id_range>
            <min>0</min>
            <max>42</max>
          </id_range>
        </domains>
        <subscribe>
          <topics>
            <topic>rt/clock</topic>
            <topic>rt/parameter_events</topic>
            <topic>rr/time_sync/describe_parametersReply</topic>
            <topic>rr/time_sync/get_parameter_typesReply</topic>
            <topic>rr/time_sync/get_parametersReply</topic>
            <topic>rr/time_sync/list_parametersReply</topic>
            <topic>rr/time_sync/set_parametersReply</topic>
            <topic>rr/time_sync/set_parameters_atomicallyReply</topic>
            <topic>rq/time_sync/describe_parametersRequest</topic>
            <topic>rq/time_sync/get_parameter_typesRequest</topic>
            <topic>rq/time_sync/get_parametersRequest</topic>
            <topic>rq/time_sync/list_parametersRequest</topic>
            <topic>rq/time_sync/set_parametersRequest</topic>
            <topic>rq/time_sync/set_parameters_atomicallyRequest</topic>
          </topics>
        </subscribe>
        <publish>
          <topics>
            <topic>rt/parameter_events</topic>
            <topic>rt/time_sync</topic>
            <topic>rq/time_sync/describe_parametersRequest</topic>
            <topic>rq/time_sync/get_parameter_typesRequest</topic>
            <topic>rq/time_sync/get_parametersRequest</topic>
            <topic>rq/time_sync/list_parametersRequest</topic>
            <topic>rq/time_sync/set_parametersRequest</topic>
            <topic>rq/time_sync/set_parameters_atomicallyRequest</topic>
            <topic>rr/time_sync/describe_parametersReply</topic>
            <topic>rr/time_sync/get_parameter_typesReply</topic>
            <topic>rr/time_sync/get_parametersReply</topic>
            <topic>rr/time_sync/list_parametersReply</topic>
            <topic>rr/time_sync/set_parametersReply</topic>
            <topic>rr/time_sync/set_parameters_atomicallyReply</topic>
          </topics>
        </publish>
      </allow_rule>
      <default>DENY</default>
    </grant>
  </permissions>
</dds>
