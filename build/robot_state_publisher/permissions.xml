<?xml version="1.0" encoding="utf-8"?>
<dds>
  <permissions>
    <grant name="robot_state_publisher">
      <subject_name>CN=robot_state_publisher</subject_name>
      <validity>
        <not_before>2013-10-26T00:00:00</not_before>
        <not_after>2023-10-26T22:45:30</not_after>
      </validity>
      <allow_rule>
        <domains>
          <id_range>
            <min>0</min>
            <max>42</max>
          </id_range>
        </domains>
        <subscribe>
          <topics>
            <topic>rt/clock</topic>
            <topic>rt/parameter_events</topic>
            <topic>rt/joint_states</topic>
            <topic>rr/robot_state_publisher/describe_parametersReply</topic>
            <topic>rr/robot_state_publisher/get_parameter_typesReply</topic>
            <topic>rr/robot_state_publisher/get_parametersReply</topic>
            <topic>rr/robot_state_publisher/list_parametersReply</topic>
            <topic>rr/robot_state_publisher/set_parametersReply</topic>
            <topic>rr/robot_state_publisher/set_parameters_atomicallyReply</topic>
            <topic>rq/robot_state_publisher/describe_parametersRequest</topic>
            <topic>rq/robot_state_publisher/get_parameter_typesRequest</topic>
            <topic>rq/robot_state_publisher/get_parametersRequest</topic>
            <topic>rq/robot_state_publisher/list_parametersRequest</topic>
            <topic>rq/robot_state_publisher/set_parametersRequest</topic>
            <topic>rq/robot_state_publisher/set_parameters_atomicallyRequest</topic>
          </topics>
        </subscribe>
        <publish>
          <topics>
            <topic>rt/parameter_events</topic>
            <topic>rt/robot_description</topic>
            <topic>rt/tf</topic>
            <topic>rt/tf_static</topic>
            <topic>rq/robot_state_publisher/describe_parametersRequest</topic>
            <topic>rq/robot_state_publisher/get_parameter_typesRequest</topic>
            <topic>rq/robot_state_publisher/get_parametersRequest</topic>
            <topic>rq/robot_state_publisher/list_parametersRequest</topic>
            <topic>rq/robot_state_publisher/set_parametersRequest</topic>
            <topic>rq/robot_state_publisher/set_parameters_atomicallyRequest</topic>
            <topic>rr/robot_state_publisher/describe_parametersReply</topic>
            <topic>rr/robot_state_publisher/get_parameter_typesReply</topic>
            <topic>rr/robot_state_publisher/get_parametersReply</topic>
            <topic>rr/robot_state_publisher/list_parametersReply</topic>
            <topic>rr/robot_state_publisher/set_parametersReply</topic>
            <topic>rr/robot_state_publisher/set_parameters_atomicallyReply</topic>
          </topics>
        </publish>
      </allow_rule>
      <default>DENY</default>
    </grant>
  </permissions>
</dds>
