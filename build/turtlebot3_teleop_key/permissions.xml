<?xml version="1.0" encoding="utf-8"?>
<dds>
  <permissions>
    <grant name="turtlebot3_teleop_key">
      <subject_name>CN=turtlebot3_teleop_key</subject_name>
      <validity>
        <not_before>2013-10-26T00:00:00</not_before>
        <not_after>2023-10-26T22:45:30</not_after>
      </validity>
      <allow_rule>
        <domains>
          <id_range>
            <min>0</min>
            <max>42</max>
          </id_range>
        </domains>
        <publish>
          <topics>
            <topic>rt/cmd_vel</topic>
          </topics>
        </publish>
      </allow_rule>
      <default>DENY</default>
    </grant>
  </permissions>
</dds>
