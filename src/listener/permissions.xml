<?xml version="1.0" encoding="utf-8"?>
<package>
    <permissions>
        <grant name="listener">
            <allow_rule>
                <ros_publish>
                    <ros_topics>
                        <ros_topic>/rosout</ros_topic>
                    </ros_topics>
                </ros_publish>
                <ros_subscribe>
                    <ros_topics>
                        <ros_topic>/chatter</ros_topic>
                    </ros_topics>
                </ros_subscribe>
            </allow_rule>
            <default>DENY</default>
        </grant>
    </permissions>
</package>
