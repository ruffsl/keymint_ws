    <ros_topic qualifier="ALLOW">
      <attachments>
        <attachment>/odom</attachment>
        <attachment>/tf</attachment>
        <attachment>/tf_static</attachment>
      </attachments>
      <permissions>
        <ros_subscribe/>
      </permissions>
    </ros_topic>
</profile>
